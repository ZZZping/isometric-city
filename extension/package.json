{
  "name": "cursor-chat-commands",
  "displayName": "Cursor Chat Commands",
  "description": "Commands for managing Cursor chat modes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cursor.switchChatToDebugMode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor.switchChatToDebugMode",
        "title": "Switch Chat to Debug Mode",
        "category": "Cursor"
      }
    ],
    "configuration": {
      "title": "Cursor Chat",
      "properties": {
        "cursor.chat.debugModelSlug": {
          "type": "string",
          "default": "",
          "description": "Model slug to use for debug mode chat"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0"
  }
}
